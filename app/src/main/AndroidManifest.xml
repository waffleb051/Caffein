<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nerdsweaponofchoice.caffeineafterburn" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseYourLevel"
            android:label="@string/title_activity_choose_your_level"
            android:parentActivityName=".StartPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="nerdsweaponofchoice.caffeineafterburn.StartPage" />
        </activity>
    </application>

</manifest>
